{
  "name": "@sola-nyan/nuxt-better-auth",
  "version": "1.0.4",
  "description": "Nuxt module for better-auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/sola-nyan/nuxt-better-auth.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.3.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.1",
    "@types/node": "^25.2.2",
    "better-auth": "^1.4.18",
    "changelogen": "^0.6.2",
    "eslint": "^10.0.0",
    "nuxt": "^4.3.1",
    "typescript": "latest",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "better-auth": "^1.4.18"
  }
}
